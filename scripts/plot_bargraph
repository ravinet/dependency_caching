set terminal svg size 544, 436 dashed rounded
fontsize = 12
set output "plts.svg"
set style fill solid 1.00 border 0
#set style histogram gap 2 lw 1
set style data histogram
set xtics rotate by -45
set grid ytics
set xlabel "Website"
set ylabel "Page load time (ms)"
set yrange [0:*]
set datafile separator " "
plot 'plt_initial_results_12_50' using 2:xtic(1) ti "Firefox" linecolor rgb "blue", \
'' using 3 ti "Polaris" lt 1 lc rgb "red"
